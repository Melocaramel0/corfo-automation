{
  "name": "corfo-automation",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "dev": "tsc --watch",
    "server": "npx ts-node server/index.ts",
    "server:dev": "npx nodemon --watch server --ext ts --exec \"npx ts-node server/index.ts\"",
    "client": "cd ui && npm run dev",
    "start": "concurrently \"npm run server:dev\" \"npm run client\"",
    "scraping": "npx ts-node scraping/extraerFormularios.ts",
    "scraping-multifase": "npx ts-node scraping/extraerFormularios.ts",
    "analisis-profundo": "npx ts-node scraping/extraerFormularios.ts -- --analisis-profundo",
    "demo": "npx ts-node ai/demoMultiFase.ts",
    "demo-simple": "npx ts-node ai/demoMultiFase.ts simple",
    "test-agente": "npx ts-node ai/test-simple.ts",
    "agente-demo": "npx ts-node ai/demo.ts",
    "mvp-hibrido": "npx ts-node ai/mvpHibrido.ts",
    "mvp-hibrido-velocidad": "npx ts-node ai/mvpHibrido.ts velocidad",
    "mvp-hibrido-produccion": "npx ts-node ai/mvpHibrido.ts produccion",
    "demo-mvp": "npx ts-node ai/demoMVP.ts",
    "demo-mvp-help": "npx ts-node ai/demoMVP.ts --help",
    "mvp-simple": "npx ts-node ai/mvpSimplificado.ts"
  },
  "keywords": [
    "corfo",
    "automation",
    "web-scraping",
    "ai",
    "forms",
    "typescript",
    "playwright"
  ],
  "author": "",
  "license": "ISC",
  "description": "Sistema automatizado para procesamiento de formularios CORFO con an√°lisis multi-fase",
  "dependencies": {
    "@playwright/test": "^1.55.0",
    "@types/express": "^4.17.21",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "n8n": "^0.167.0",
    "playwright": "^1.56.1",
    "typescript": "^5.8.3",
    "vite": "^7.1.12"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.9.1",
    "@types/request": "^2.48.13",
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2"
  }
}
